{
	"manifest_version": 3,
	"name": "cw",
	"short_name": "cw",
	"version": "1.0.0",
	"description": "CW Generator",
	"permissions": [
		"tabs",
		"activeTab",
		"alarms",
		"tabGroups",
		"scripting",
		"notifications",
		"declarativeNetRequestWithHostAccess",
		"declarativeNetRequestFeedback",
		"declarativeContent",
		"storage",
		"contextMenus",
		"sessions"
	],
	// "icons": {
	// 	"16": "images/icon.png",
	// 	"24": "images/icon.png",
	// 	"32": "images/icon.png"
	// },
	"action": {
		"default_popup": "index.html?#/popup",
		"default_title": "Angular Chrome Extension"
		// "default_icon": {
		// 	"16": "images/icon.png",
		// 	"24": "images/icon.png",
		// 	"32": "images/icon.png"
		// }
	},
	"options_page": "index.html?#/options",
	"content_scripts": [
		{
			"js": ["contentPage.js"],
			"matches": ["<all_urls>"]
		}
	],
	"background": {
		"service_worker": "serviceWorker.js"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	}
}
